{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": [
            "last 2 versions",
            "safari >= 7"
          ]
        },
        "modules": false,
        "useBuiltIns": false,
        "loose": true,
        "debug": false
      }
    ]
  ],
  "plugins": [
    "transform-runtime"
  ],
  "sourceMaps": true
}
